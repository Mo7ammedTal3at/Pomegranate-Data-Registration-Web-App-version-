@model IEnumerable<NoorEl7abeebCompanyWebApp.Models.MyModels.Export>
<table id="customers" class="table table-bordered table-striped">
    <thead >
        <tr style="text-align: right">
            <th>
                @Html.DisplayNameFor(model => model.Customer.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BoxType.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Quantity)
            </th>
            <th class="noPrint">
                @Html.DisplayNameFor(model => model.Notes)
            </th>
            @*<th></th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Customer.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BoxType.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td class="bg-success">
                    @Html.DisplayFor(modelItem => item.Quantity)
                </td>
                <td class="noPrint">
                    @Html.DisplayFor(modelItem => item.Notes)
                </td>
                @*<td>
                    @Html.ActionLink("تعديل", "Edit", new { id = item.Id }, new { @class = "btn btn-warning" }) |
                    @Html.ActionLink("تفاصيل", "Details", new { id = item.Id }, new { @class = "btn btn-info" }) |
                    @Html.ActionLink("مسح", "Delete", new { id = item.Id }, new { @class = "btn btn-danger" })
                </td>*@
            </tr>
        }
    </tbody>
</table>
